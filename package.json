{
  "name": "ros",
  "displayName": "ROS",
  "icon": "assets/icon.png",
  "description": "Development support for Robot Operating System (ROS)",
  "version": "0.0.1",
  "publisher": "ajshort",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajshort/vscode-ros.git"
  },
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "category": "ROS",
        "command": "ros.showMasterStatus",
        "title": "Show Master Status"
      },
      {
        "category": "ROS",
        "command": "ros.startCore",
        "title": "Start Core"
      },
      {
        "category": "ROS",
        "command": "ros.stopCore",
        "title": "Stop Core"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ROS",
      "properties": {
        "ros.distro": {
          "type": "string",
          "description": "ROS distro name to use (kinetic, jade, ...)"
        }
      }
    },
    "languages": [
      {
        "id": "xml",
        "extensions": [
          "launch",
          "srdf",
          "test",
          "urdf",
          "xacro"
        ]
      },
      {
        "id": "yaml",
        "extensions": [
          "rviz"
        ]
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "underscore": "^1.8.3",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/underscore": "^1.7.36",
    "@types/xmlrpc": "^1.3.2",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
